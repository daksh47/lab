   "source": [
    "import numpy as np  \n",
    "import matplotlib.pyplot as plt  \n",
    "from scipy.stats import norm, poisson, binom"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "57cb1s3s1",
   "metadata": {},
   "source": [
    "### PROGRAM 1"
   ]
  }
  {
   "cell_type": "markdown",
   "id": "9897bf5b",
   "metadata": {},
   "source": [
    "### SIMPLE PROBABILITY"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "dcf8a8a9",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "probability of rolling a 4 0.16666666666666666\n"
     ]
    }
   ],
   "source": [
    "total_outcomes=6\n",
    "favorable_outcomes=1\n",
    "probability=favorable_outcomes/total_outcomes\n",
    "print(f\"probability of rolling a 4 {probability}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "bd0e9df1",
   "metadata": {},
   "source": [
    "### NORMAL DISTRIBUTION"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "7f3f0cff",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 576x432 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "mean = 50 \n",
    "std_dev = 10 \n",
    "samples = np.random.normal(mean, std_dev, 1000) \n",
    "plt.figure(figsize=(8, 6)) \n",
    "plt.hist(samples, bins=30, density=True, alpha=0.6, color='blue') \n",
    "x = np.linspace(mean - 4*std_dev, mean + 4*std_dev, 100) \n",
    "plt.plot(x, norm.pdf(x, mean, std_dev), 'r-', lw=2, label='Normal Distribution') \n",
    "plt.title('Normal Distribution Example (Quality Control)') \n",
    "plt.xlabel('Values') \n",
    "plt.ylabel('Probability Density') \n",
    "plt.legend() \n",
    "plt.grid(True) \n",
    "plt.show() "
   ]
  },
  {
   "cell_type": "markdown",
   "id": "e98accfd",
   "metadata": {},
   "source": [
    "### POISSON DISTRIBUTION"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "af1abe4f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Probability of 3 events occurring in an hour: 0.1403738958142805\n"
     ]
    }
   ],
   "source": [
    "lambda_param = 5  \n",
    "x = 3  \n",
    "prob_3_events = poisson.pmf(x, lambda_param)  \n",
    "print(f\"Probability of 3 events occurring in an hour: {prob_3_events}\")"
   ]
  },
  {
